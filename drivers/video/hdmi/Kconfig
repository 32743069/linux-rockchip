#
# Display drivers configuration
#
menu "HDMI"
config HDMI
	tristate "HDMI support"

if HDMI

config HDMI_OLD
	default y
	bool "old hdmi support"
	help
		nothing
if HDMI_OLD
source "drivers/video/hdmi/hdmi-old/Kconfig"
endif

config HDMI_NEW
	bool "new hdmi support"
	help
		nothing
if HDMI_NEW
source "drivers/video/hdmi/hdmi-new/Kconfig"
endif

config HDMI_DEBUG
	bool "hdmi debug"

endif

endmenu
